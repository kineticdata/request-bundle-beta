<% if (!ThemeHelper.isBlank(UserContext.getErrorMessage())) { %>
<div class="errorMessage">
    <jsp:getProperty name="UserContext" property="errorMessage"/>
</div>
<jsp:setProperty name="UserContext" property="errorMessage" value=""/>
<% } %>
<!-- END {themeRoot}/jsp/pageFragments/sessionMessage.jsfp -->